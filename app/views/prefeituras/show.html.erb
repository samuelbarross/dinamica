<%- model_class = Prefeitura -%>
<div class="wrapper wrapper-content animated fadeInRight">
  <div class="ibox ">
    <div class="ibox-title">
      <h2>Dados</h2>
    </div>
    <div class="ibox-content">
              <p>
          <strong><%= model_class.human_attribute_name( :nome) %>:</strong>
          <%= @prefeitura.nome %>
        </p>
              <p>
          <strong><%= model_class.human_attribute_name( :cidade) %>:</strong>
          <%= @prefeitura.cidade %>
        </p>
          </div>
  </div>
</div>
